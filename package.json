{
  "name": "angularfire-material-dashboard",
  "description": "Cross platform full authentication system and dashboard implemented with Angular 6, Firebase, Material Design, NgRx, Electron and Nativescript.",
  "version": "1.0.0",
  "author": "Razvan Tomegea",
  "homepage": "https://angularfire-material-dashboard.firebaseapp.com",
  "license": "MIT",
  "private": false,
  "main": "electron-main.js",
  "nativescript": {
    "id": "org.nativescript.angularfirematerialdashboard"
  },
  "scripts": {
    "android": "tns run android --bundle",
    "build": "npm run lint && ng build --prod --outputPath=dist",
    "clear:nix": "rimraf platforms/angularfire-material-dashboard-linux-x64 && rimraf platforms/angularfire-material-dashboard-linux-ia32",
    "clear:osx": "rimraf platforms/angularfire-material-dashboard-darwin-x64 && rimraf platforms/angularfire-material-dashboard-mas-x64",
    "clear:win": "rimraf platforms/angularfire-material-dashboard-win32-x64 && rimraf platforms/angularfire-material-dashboard-win32-ia32",
    "deploy": "firebase deploy",
    "e2e": "ng e2e",
    "electron": "electron .",
    "init": "firebase init",
    "ios": "tns run ios --bundle",
    "lint": "ng lint",
    "ng": "ng",
    "pack:nix": "npm run clear:nix && copy electron-main.js dist && copy package.json dist && electron-packager ./dist/ --platform=linux --arch=ia32,x64 --asar --out=platforms",
    "pack:osx": "npm run clear:osx && copy electron-main.js dist && copy package.json dist && electron-packager ./dist/ --platform=darwin,mas --arch=x64 --asar --out=platforms",
    "pack:win": "npm run clear:win && copy electron-main.js dist && copy package.json dist && electron-packager ./dist --platform=win32 --arch=ia32,x64 --asar --out=platforms",
    "postinstall": "copy lib/convert-units/index.js node_modules/convert-units/lib",
    "start": "ng serve -o",
    "test": "ng test",
    "update": "ng update --all"
  },
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.3.2",
    "@angular/cli": "6.2.6",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/fire": "5.1.0",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/material": "6.3.2",
    "@angular/material-moment-adapter": "6.4.7",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@fortawesome/fontawesome-svg-core": "1.2.8",
    "@fortawesome/free-brands-svg-icons": "5.5.0",
    "@fortawesome/free-regular-svg-icons": "5.5.0",
    "@fortawesome/free-solid-svg-icons": "5.5.0",
    "@ngrx/effects": "6.1.2",
    "@ngrx/router-store": "6.1.2",
    "@ngrx/store": "6.1.2",
    "@ngrx/store-devtools": "6.1.2",
    "@ngx-loading-bar/core": "2.2.0",
    "@ngx-loading-bar/http-client": "2.2.0",
    "@ngx-loading-bar/router": "2.2.0",
    "@trademe/ng-defer-load": "3.0.0",
    "amazing-time-picker": "1.8.0",
    "angular-particle": "1.0.4",
    "chart.js": "2.7.3",
    "classlist.js": "1.1.20150312",
    "convert-units": "2.3.4",
    "core-js": "2.5.4",
    "firebase": "5.5.8",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "intersection-observer": "0.5.1",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "nativescript-angular": "6.1.0",
    "nativescript-theme-core": "1.0.4",
    "ng-lazyload-image": "5.0.0",
    "ng2-charts": "1.6.0",
    "ng2-tel-input": "1.0.16",
    "ngx-markdown": "7.1.4",
    "ngx-spinner": "6.1.2",
    "reading-time": "1.1.3",
    "reflect-metadata": "0.1.8",
    "rxjs": "6.2.2",
    "rxjs-compat": "6.2.1",
    "tns-core-modules": "4.2.0",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.0",
    "@angular/cli": "6.2.0",
    "@angular/compiler-cli": "6.0.3",
    "@nativescript/schematics": "0.3.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "copy": "0.3.2",
    "electron": "9.1.0",
    "electron-packager": "12.2.0",
    "firebase-tools": "6.5.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "nativescript": "5.3.0",
    "nativescript-dev-sass": "1.6.0",
    "nativescript-dev-typescript": "0.7.0",
    "nativescript-dev-webpack": "0.20.0",
    "protractor": "5.4.2",
    "rimraf": "2.6.2",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "webpack": "4.24.0"
  }
}
